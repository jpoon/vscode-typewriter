	    {"key": "numpad9","command": "extension.typewriter_9","when": "editorTextFocus"},
	    {"key": "backspace","command": "extension.typewriter_backspace", "when": "editorTextFocus"},
	    {"key": "space", "command": "extension.typewriter_space", "when": "editorTextFocus" },
	    {"key": "tab", "command": "extension.typewriter_tab", "when": "editorTextFocus" }
    ]},
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
