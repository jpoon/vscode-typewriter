{
  "name": "vscode-typewriter",
  "displayName": "vscode-typewriter",
  "description": "Make VS Code sound like a typewriter",
  "version": "0.0.6",
  "publisher": "jasonpoon",
  "engines": {
    "vscode": "^1.4.0"
  },
  "license": "MIT",
  "keywords": [
    "typewriter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jpoon/vscode-typewriter.git"
  },
  "homepage": "https://github.com/jpoon/vscode-typewriter",
  "bugs": {
    "url": "https://github.com/jpoon/vscode-typewriter/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
	    {"key": "a","command": "extension.typewriter_a","when": "editorTextFocus"},
	    {"key": "b","command": "extension.typewriter_b","when": "editorTextFocus"},
	    {"key": "c","command": "extension.typewriter_c","when": "editorTextFocus"},
